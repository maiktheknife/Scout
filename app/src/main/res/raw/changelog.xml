    <changelogversion versionName="1.8.2" changeDate="13/10/2015">
        <changelogtext>Kontakte werden nun bei Enge zusammen dargestellt</changelogtext>
        <changelogtext>Pull-to-Refresh Support in den Kontaktübersicht </changelogtext>
    </changelogversion>

    <changelogversion versionName="1.8.1" changeDate="15/09/2015">
        <changelogtext>Freundeübersicht mit mehr Optionen und einem hübschen Layout</changelogtext>
        <changelogtext>neuer Changelog Darstellung</changelogtext>
        <changelogtext>License Übersicht in den Einstellungen</changelogtext>
    </changelogversion>

    <changelogversion versionName="1.8" changeDate="27/08/2015">
        <changelogtext>Anpassung auf das neue Backend (REST &#38; PostGres) </changelogtext>
        <changelogtext>weniger Datenverbrauch und schnellers</changelogtext>
        <changelogtext>HeatMap als neue Verlaufsform</changelogtext>
        <changelogtext>Einführung in die App hinzugefügt</changelogtext>
        <changelogtext>Wetterdaten Übersichtoption hinzugefügt</changelogtext>
        <changelogtext>[b][font color=red]Dev:[/font][/b]Nutzung der neuen AppCompat &#38; Design Libary:</changelogtext>
    </changelogversion>

    <changelogversion versionName="1.7.1" changeDate="10/06/2015">
        <changelogtext>Fotos aus G+ hinzugefügt</changelogtext>
        <changelogtext>Problem beim Start behoben</changelogtext>
        <changelogtext>Design Improvements</changelogtext>
    </changelogversion>

    <changelogversion versionName="1.7" changeDate="24/05/2015">
        <changelogtext>Google Plus Login</changelogtext>
        <changelogtext>[font color=red]Achievements &#38; Leaderboard hinzugefügt[/font]</changelogtext>
        <changelogtext>Backend Probleme behoben</changelogtext>
        <changelogtext>Die App achtet jetzt mehr auf den Zustand des Gerätes</changelogtext>
    </changelogversion>

    <changelogversion versionName="1.6" changeDate="23/03/2015">
        <changelogtext>Android 5 Support</changelogtext>
        <changelogtext>Bugs behoben</changelogtext>
    </changelogversion>

    <changelogversion versionName="1.5" changeDate="14/09/2013">
        <changelogtext>Bug bei den Ping Einstellugen behoben (Thx to Mr Meuser)</changelogtext>
        <changelogtext>Aktualisierung immer verfügbar </changelogtext>
        <changelogtext>Cache der Adressen in sqllite</changelogtext>
        <changelogtext>ChangeLog Dialog eingeführt</changelogtext>
    </changelogversion>

    <changelogversion versionName="&lt;1.5" changeDate="28/08/2013">
